{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lcmondini/node-typescript-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Lucas Mondini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lcmondini/node-typescript-api/issues"
  },
  "homepage": "https://github.com/lcmondini/node-typescript-api#readme",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.6.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.0.0-pre.58",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
